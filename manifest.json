{
  "manifest_version": 2,
  "name": "Better Twitter Sidebar",
  "version": "1.0",
  "description": "Removes an element from Twitter.com",
  "permissions": ["https://twitter.com/*"],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
